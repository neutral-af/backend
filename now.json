{
    "version": 2,
    "name": "backend",
    "alias": ["api.neutral.af"],
    "builds": [
        { "src": "cmd/zeit/handler.go", "use": "@now/go" }
    ],
    "routes": [
        { "src": "/graphql", "dest": "cmd/zeit/handler.go" }
    ],
    "env": {
        "ENVIRONMENT": "prod",
        "CLOVERLY_API_KEY": "@cloverly_api_key",
        "HONEYCOMB_API_KEY": "@honeycomb_api_key",
        "STRIPE_SECRET_KEY": "@stripe_secret_key"
    },
    "regions": ["bru"],
    "public": false
}